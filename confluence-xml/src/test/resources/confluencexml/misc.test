            <entry>
              <string>syntax</string>
              <org.xwiki.rendering.syntax.Syntax>
                <type>
                  <name>XWiki</name>
                  <id>xwiki</id>
                  <variants class="empty-list"/>
                </type>
                <version>2.1</version>
              </org.xwiki.rendering.syntax.Syntax>
